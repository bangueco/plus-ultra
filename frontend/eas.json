    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "extends": "production",
      "developmentClient": true,
      "distribution": "internal",
      "env": {
        "EXPO_PUBLIC_API_URL": "http://192.168.100.4:3000/api"
      }
    },
    "preview": {
      "extends": "production",
      "distribution": "internal"
    },
    "production": {
      "node": "20.15.1",
      "env": {
        "EXPO_PUBLIC_API_URL": "http://192.168.100.4:3000/api"
      },
      "android": {
        "buildType": "apk"
      },
      "autoIncrement": true
    }
  },
  "submit": {
    "production": {}
  }
}
